package map;

import unit.Unit;

public class UnitMap {
	
	Unit[][] map;
	
	public UnitMap(ArrayList<ArrayList<Unit>> UnitList)
	{
		
	}
	
	public Unit[][] getUnitMap()
	{
		return map;
	}
	
	public Unit[] getUnitMap(int y)
	{
		return map[y];
	}
	
	public Unit getUnitMap(int y,int x)
	{
		return map[y][x];
	}
	
}
	

